{
  "name": "angulon",
  "productName": "Angulon",
  "author": "Stein Jonker",
  "version": "1.0.0",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Staijn1/angulon.git"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "postinstall": "./node_modules/.bin/electron-rebuild --target=2.0.4",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron:windows": "electron-packager . angulon --overwrite  --platform=win32 --arch=all --prune=true --out=release-builds",
    "electron:linux": "electron-packager . angulon --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/platform-server": "11.0.0",
    "@angular/router": "^11.0.0",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@jaames/iro": "^5.2.3",
    "@types/serialport": "^6.0.3",
    "audiomotion-analyzer": "^2.4.0",
    "bootstrap": "^4.1.2",
    "core-js": "^2.6.5",
    "electron-json-config": "^1.5.3",
    "jquery": "3.5.1",
    "ng2-nouislider": "1.8.2",
    "nouislider": "14.6.2",
    "p5": "^1.1.9",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.4.0",
    "serialport": "^6.2.1",
    "tslib": "^2.0.0",
    "uglify-js": "^3.3.26",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.0",
    "@angular/cli": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/bootstrap": "3.3.32",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "2.0.4",
    "electron-builder": "20.14.7",
    "electron-packager": "^15.1.0",
    "electron-rebuild": "^1.8.1",
    "electron-reload": "^1.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~7.0.0",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.5",
    "update": "^0.7.4",
    "wait-on": "^2.1.0",
    "webpack": "^4.31.0"
  }
}
