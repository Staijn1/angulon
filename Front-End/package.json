{
  "name": "angulon",
  "productName": "Angulon",
  "author": "Stein Jonker",
  "version": "1.0.0",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Staijn1/angulon.git"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "postinstall": "node rebuild.js",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall && npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --disable-host-check",
    "electron:tsc": "tsc",
    "electron:serve": "npm run electron:tsc && electron . --serve",
    "electron:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --icon=\"favicon.ico\"",
    "electron:windows": "npm run build:prod && electron-packager . angulon --overwrite  --platform=win32 --arch=all --prune=true --out=release-builds --icon=\"favicon.ico\"",
    "electron:linux": "electron-packager . angulon --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds --icon=\"favicon.ico\"",
    "compile-watch": "tsc -w",
    "e2e": "npm run postinstall && npm run electron:tsc && mocha",
    "test": "ng test --code-coverage --watch false",
    "test:watch": "ng test --code-coverage --watch true"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@jaames/iro": "5.3.1",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "audiomotion-analyzer": "^3.3.0",
    "bootstrap": "^5.0.1",
    "core-js": "3.8.0",
    "gsap": "^3.5.1",
    "ng2-nouislider": "^1.8.2",
    "nouislider": "^14.6.3",
    "reconnecting-websocket": "^4.4.0",
    "rxjs": "^6.5.2",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "11.0.0",
    "@types/bootstrap": "5.0.0",
    "@types/jasmine": "^3.6.2",
    "@types/node": "14.0.27",
    "codelyzer": "6.0.1",
    "electron": "11.0.0",
    "electron-builder": "22.9.1",
    "electron-packager": "15.1.0",
    "electron-rebuild": "2.3.4",
    "electron-reload": "1.5.0",
    "jasmine": "^3.6.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-instrumenter": "^1.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^2.1.1",
    "spectron": "^13.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.5",
    "webpack": "4.31.0"
  }
}
